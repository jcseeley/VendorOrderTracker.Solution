@{
  Layout = "_Layout";
}

<h1>Add A Vendor</h1>
<form action="/vendors" method="post">
  <input id="vendorName" name="vendorName" type="text" placeholder="Vendor Name"><br>
  <input id="vendorDescription" name="vendorDescription" type="text" placeholder="Description of Vendor"><br>
  <button type="submit">Add Vendor</button>
</form>
<a href="/vendors">Back to Vendor List</a>